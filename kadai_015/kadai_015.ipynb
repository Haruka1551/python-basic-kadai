        "#課題_015\n",
        "class Human:\n",
        "  def __init__(self,name,age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "    print(f\"name:{self.name},Age:{self.age}\")\n",
        "\n",
        "human1 = Human(\"A\",20)\n",
        "human2 = Human(\"B\",25)\n",
        "\n",
        "human1.printinfo()\n",
        "human2.printinfo()\n"
      ]
